<div id="<%= dom_id atividade %>">
  <p>
    <strong>Nome:</strong>
    <%= atividade.nome %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= atividade.data %>
  </p>

  <p>
    <strong>Hora inicio:</strong>
    <%= atividade.hora_inicio %>
  </p>

  <p>
    <strong>Hora final:</strong>
    <%= atividade.hora_final %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= atividade.status %>
  </p>

</div>
